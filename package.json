  "scripts": {
    "local": "env-cmd -f .env.local react-scripts start",
    "start": "env-cmd -f .env.development react-scripts start",
    "dev": "env-cmd -f .env.local react-scripts start",
    "staging": "env-cmd -f .env.staging react-scripts build",
    "s": "git pull && npm install && env-cmd -f .env.staging react-scripts build && rm -rf ../public_html/* && mv build/* ../public_html/",
    "p": "git pull && npm install && env-cmd -f .env.production react-scripts build && rm -rf ../public_html/* && mv build/* ../public_html/",
    "build": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/redux-logger": "^3.0.8",
    "node-sass": "4.14.1"
  }
